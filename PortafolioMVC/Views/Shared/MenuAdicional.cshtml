@model Usuario


<div class="options-container-fixed">
    <div class="options">

        <div class="row">

            <div class="col-sm-12">
                <a href="#" class="btn btn-danger">
                    <i class="fa fa-file-pdf-o"></i><span class="hidden-xs hidden-sm">Descargar en PDF</span>
                </a>

                <button class="btn btn-primary">
                    <i class="fa fa-print"></i><span class="hidden-xs hidden-sm">Imprimir</span>
                </button>


                <button class="btn btn-default" data-toggle="modal" data-target="">
                    <i class="fa fa-envelope-o"></i><span class="hidden-xs hidden-sm">Contactar</span>
                </button>

                @if (Model.facebook!=null)
                {
                    <a target="_blank" href="@Model.facebook" title="Facebook" class="btn btn-default"> <i class="fa fa-facebook-official"></i> </a>
                }
                @if (Model.twitter != null)
                {
                    <a target="_blank" href="@Model.twitter" title="Twitter" class="btn btn-default"> <i class="fa fa-twitter"></i> </a>
                }
                
                @if (Model.youtube!=null)
                {
                     <a target="_blank" href="@Model.youtube" title="Youtube" class="btn btn-default"> <i class="fa fa-youtube"></i> </a>
                }
               
            </div>

        </div>

    </div>


</div>



